# get the package
import altar
import altar.cuda
import altar.models.seismic.cuda

# make a specialized app that uses this model by default
class cudaCascaded(altar.shells.cudaapplication, family='altar.applications.cudacascaded'):
    """
    A specialized AlTar application that exercises the Cascaded inversion
    """

    # user configurable state
    model = altar.cuda.models.model()
    model.default = altar.models.seismic.cuda.cascaded()
    model.doc = "the AlTar model to sample"

# bootstrap
if __name__ == "__main__":
    # build an instance of the default app
    app = cudaCascaded(name="cudacascaded")
    # invoke the main entry point
    status = app.run()
    # share
    raise SystemExit(status)


# end of file
